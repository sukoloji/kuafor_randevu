@model kuafor_randevuV2.Models.calisanlar
@{
    Layout = null;
}

@using (Html.BeginForm("AddOrEditPersonnel", "Admin", FormMethod.Post, new { onsubmit = "return SubmitForm(this)" }))
{
    @Html.HiddenFor(model => model.id)
    <div class="form-group">
        @Html.LabelFor(model => model.calisan_adi, new { @class = "control-label" })
        @Html.EditorFor(model => model.calisan_adi, new { htmlAttributes = new { @class = "form-control" } })
    </div>
    <div class="form-group">
        <input type="submit" value="Kaydet" class="btn btn-primary" />
    </div>
}


